{
	"manifest_version": 2,
	"name": "C2Geo Kit",
	"version": "0.1",
  "browser_action": {
    "default_icon": {                    // optional
      "19": "icon_19.png",           // optional
      "38": "icon_38.png"            // optional
    },
    "default_title": "C2Geo Kit",      // optional; shown in tooltip
    "default_popup": "popup.html"        // optional
  },
  "content_scripts": [
  {
    "matches": ["http://*.fds.com/shop*"],
    "js": ["jquery-1.9.1.js", "require.js", "inject3.js"]
  }
	]
}